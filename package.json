{
  "name": "disk-management",
  "version": "0.3.2",
  "description": "Module/application for management your disk",
  "main": "index.js",
  "license": "MIT",
  "author": "Giap Dong",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently 'npm:api' 'npm:client'",
    "api": "cross-env NODE_ENV=development nodemon ./webapp/api/bin/www",
    "client": "cross-env NODE_ENV=development node ./webapp/client/bin/www",
    "build": "cross-env NODE_ENV=production webpack --config ./webapp/client/webpack.prod.js",
    "dev:ts": "npm run build:ts && npm run serve:api",
    "serve:api": "node build/api/index.js",
    "build:ts": "tsc",
    "example:ts": "ts-node examples/lib.example.ts",
    "test:ts": "jest --detectOpenHandles"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "colors": "^1.4.0",
    "commander": "^6.1.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "open": "^7.2.1",
    "ora": "^4.0.5",
    "response-format": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "ant-design-vue": "^1.6.5",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "express-generator": "^4.16.1",
    "file-loader": "^6.1.0",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.6.3",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "mini-css-extract-plugin": "^0.11.1",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.5.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "bin": {
    "disk-management": "./bin/cli.js"
  },
  "keywords": [
    "disk-management",
    "storage",
    "cli"
  ],
  "typings": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giapdong/disk-management.git"
  },
  "bugs": {
    "url": "https://github.com/giapdong/disk-management/issues"
  },
  "homepage": "https://github.com/giapdong/disk-management#readme",
  "files": [
    "bin/*.js",
    "lib",
    "webapp/api/*",
    "webapp/client/bin/*",
    "webapp/client/dist/*",
    "webapp/client/app.js",
    "types/*.d.ts"
  ]
}
